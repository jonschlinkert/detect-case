{
  "name": "detect-case",
  "description": "Detects the casing of the input string (camelcase, lowercase, snakecase etc).",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/jonschlinkert/detect-case",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/detect-case",
  "bugs": {
    "url": "https://github.com/jonschlinkert/detect-case/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "eslint": "npx eslint --ext .ts .",
    "test": "ts-mocha -r esbuild-register 'test/**/*.test.ts'",
    "tsup": "npm run clean && npx tsup"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "esbuild-register": "^3.6.0",
    "eslint": "^8.57.0",
    "gulp-format-md": "^2.0.0",
    "prettier": "^3.4.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "camel",
    "camelcase",
    "case-sensitive",
    "case",
    "detect",
    "exec",
    "lowercase",
    "match",
    "matches",
    "occurrence",
    "occurrences",
    "pattern",
    "patterns",
    "regex",
    "regular expression",
    "replace",
    "replacement",
    "snake",
    "snakecase",
    "string",
    "substring",
    "title",
    "titlecase",
    "uppercase"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
